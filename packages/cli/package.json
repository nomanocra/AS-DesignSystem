{
  "name": "@as-design-system/cli",
  "version": "0.1.0",
  "description": "CLI tool to add AS Design System components to your project",
  "type": "module",
  "bin": {
    "asds": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "sync": "node scripts/sync-templates.js",
    "build": "npm run sync && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run sync"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "cosmiconfig": "^9.0.0",
    "zod": "^3.22.4",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1"
  },
  "keywords": [
    "design-system",
    "cli",
    "components",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/as-design-system"
  },
  "license": "MIT"
}
