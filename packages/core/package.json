{
  "name": "@as-design-system/core",
  "version": "0.1.0",
  "description": "Composants React du design system AS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Button.css": "./dist/Button.css",
    "./IconButton.css": "./dist/IconButton.css",
    "./Icon.css": "./dist/Icon.css",
    "./Tab.css": "./dist/Tab.css",
    "./ToolIcons.css": "./dist/ToolIcons.css",
    "./colors.css": "./dist/colors.css",
    "./typography.css": "./dist/typography.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate-icons": "node scripts/generate-icon-imports.js"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}